{
  "name": "tabris-es6-test",
  "version": "0.0.1",
  "main": "src/main.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipsesource/tabris-es6-test.git"
  },
  "scripts": {
    "lint": "jshint",
    "build:transpile": "rm -r build/ && mkdir -p build/src/ && cp -a package.json node_modules/ src/ build/ && babel --compact false --out-dir build/src/ src/",
    "build": "webpack --config webpack.config.js",
    "start": "cd build && hs -c-1"
  },
  "dependencies": {
    "tabris": "https://tabrisjs.com/downloads/nightly/tabris.tgz"
  },
  "devDependencies": {
    "promise": "^6.1.0",
    "webpack": "^1.12.14",
    "whatwg-fetch": "^0.11.0"
  }
}
